  <Font name="FarmLogFont" font="Fonts\FRIZQT__.TTF" virtual="true">
    <FontHeight>
      <AbsValue val="12"/>
    </FontHeight>
  </Font>

  <Font name="FarmLogSmallFont" font="Fonts\FRIZQT__.TTF" virtual="true">
    <FontHeight>
      <AbsValue val="8"/>
